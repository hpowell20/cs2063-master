<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mobiledev.unb.ca.lab3intents" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="mobiledev.unb.ca.lab3intents.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="mobiledev.unb.ca.lab3intents.ExternalActivityCalls"
            android:label="@string/title_activity_activity_two"
            android:parentActivityName="mobiledev.unb.ca.lab3intents.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="mobiledev.unb.ca.lab3intents.MainActivity" />
        </activity>
    </application>

</manifest>
